{
  "name": "rc-revol-community-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "lint:fix": "eslint --fix --ext .tsx,.ts src",
    "types-check": "tsc --project tsconfig.json --noEmit --noEmitHelpers --noErrorTruncation --strict",
    "gen-api:game": "npx openapi --input https://rc-revo-game-prod.herokuapp.com/rc_revo/docs-json --output ./src/shared/api/generated/game -c axios",
    "gen-api:auth": "npx openapi --input https://rc-revo-auth-stage.herokuapp.com/auth/docs-json --output ./src/shared/api/generated/auth -c axios"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@tanstack/react-query": "^5.8.7",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "date-fns": "^2.30.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.1",
    "openapi-typescript-codegen": "^0.25.0",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-perfectionist": "^2.4.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
